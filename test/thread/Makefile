### Variables: ###

CPPDEPS = -MT$@ -MF`echo $@ | sed -e 's,\.o$$,.d,'` -MD -MP
THREAD_TEST_CXXFLAGS =  -I../../include -g  $(CPPFLAGS) $(CXXFLAGS)
THREAD_TEST_OBJECTS =  \
	thread_test_main.o \
	thread_test_thread_attribute.o \
	thread_test_thread.o

### Conditionally set variables: ###



### Targets: ###

all: thread_test

install: 

uninstall: 

clean: 
	rm -f ./*.o
	rm -f ./*.d
	rm -f thread_test

thread_test: $(THREAD_TEST_OBJECTS)
	$(CXX) -o $@ $(THREAD_TEST_OBJECTS)  -g $(LDFLAGS)  -lpthread

thread_test_main.o: ./main.cpp
	$(CXX) -c -o $@ $(THREAD_TEST_CXXFLAGS) $(CPPDEPS) $<

thread_test_thread_attribute.o: ./thread_attribute.cpp
	$(CXX) -c -o $@ $(THREAD_TEST_CXXFLAGS) $(CPPDEPS) $<

thread_test_thread.o: ./thread.cpp
	$(CXX) -c -o $@ $(THREAD_TEST_CXXFLAGS) $(CPPDEPS) $<

.PHONY: all install uninstall clean


